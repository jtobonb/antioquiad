<div class="page-header">
        <h1>Subregiones</h1>
</div>

<div class="row-fluid">
    <div class="span12">
       <a href="/subregions/new" class="btn btn btn-success">Ingrese una Subregi√≥n</a>
       
    </div>
</div>
<br />


<table class="table table-bordered table-striped">
  <tr>
    <th>nombre</th>
    <th>codigo</th>
    <th><div align="center"><a href="#" title="Buscar"><i class="icon-search"></i></a></div></th>
    <th><div align="center"><a href="#" title="Editar"><i class="icon-edit"></i></a></div></th>
    <th><div align="center"><a href="#" title="Eliminar"><i class="icon-remove"></i></a></div></th>

  </tr>

<% @subregions.each do |subregion| %>
  <tr>
  <td><a href="/subregions/subregions_reporte/<%= subregion.nombre_subregion %>"><%= subregion.nombre_subregion %></a></td>
  <td><%= subregion.cod_subregion %></td>
    <td><%= link_to 'Mostrar', subregion %></td>
    <td><%= link_to 'Editar', edit_subregion_path(subregion) %></td>
    <td><%= link_to 'Eliminar', subregion, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
  </tr>
<% end %>
</table>