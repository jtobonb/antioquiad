<h1>Listing equipment</h1>

<table>
  <tr>
    <th>Dane sede</th>
    <th>Tipo</th>
    <th>Origen</th>
    <th>Fecha entrega</th>
    <th>Nombre</th>
    <th>Fabricante</th>
    <th>Modelo</th>
    <th>Serial</th>
    <th>Procesador</th>
    <th>Memoria ram</th>
    <th>Disco duro</th>
    <th>Display</th>
    <th>Sist operativo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @equipment.each do |equipment| %>
  <tr>
    <td><%= equipment.dane_sede %></td>
    <td><%= equipment.tipo %></td>
    <td><%= equipment.origen %></td>
    <td><%= equipment.fecha_entrega %></td>
    <td><%= equipment.nombre %></td>
    <td><%= equipment.fabricante %></td>
    <td><%= equipment.modelo %></td>
    <td><%= equipment.serial %></td>
    <td><%= equipment.procesador %></td>
    <td><%= equipment.memoria_ram %></td>
    <td><%= equipment.disco_duro %></td>
    <td><%= equipment.display %></td>
    <td><%= equipment.sist_operativo %></td>
    <td><%= link_to 'Show', equipment %></td>
    <td><%= link_to 'Edit', edit_equipment_path(equipment) %></td>
    <td><%= link_to 'Destroy', equipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

