<div class="page-header">
        <h1>Sedes</h1>
</div>

<div class="row-fluid">
    <div class="span12">
        <%= link_to 'Ingresar una Sede Educativa', new_sede_path, :class => "btn btn btn-success" %>
    </div>
</div>
<br />

<table id="tablaSedes" class="table table-bordered table-striped">
  <tr>
    <th>Dane sede</th>
    <!--
    <th>Dane establecimiento</th>
    <th>Localidad</th>
    -->
    <th>Nombre</th>
    <th>Zona</th>
    <!--
    <th>Calendario</th>
    <th>Jornada</th>
    <th>Dirección</th>
    <th>Teléfono</th>
    <th>Correo</th>
    -->
    <th><div align="center"><a href="#" title="Buscar"><i class="icon-search"></i></a></div></th>
    <th><div align="center"><a href="#" title="Editar"><i class="icon-edit"></i></a></div></th>
    <th><div align="center"><a href="#" title="Eliminar"><i class="icon-remove"></i></a></div></th>
  </tr>

<% @sedes.each do |sede| %>
  <tr>
  <td><a href="/sedes/sedes_reporte/<%= sede.dane_sede %>"><%= sede.dane_sede %></a></td>
    <!--
    <td><%= sede.dane_establec %></td>
    <td><%= sede.localidad %></td>
    -->
    <td><%= sede.nombre %></td>
    <td><%= sede.zona %></td>
    <!--
    <td><%= sede.calendario %></td>
    <td><%= sede.jornada %></td>
    <td><%= sede.direccion %></td>
    <td><%= sede.telefono %></td>
    <td><%= sede.correo %></td>
    -->
    <td><%= link_to 'Mostrar', sede %></td>
    <td><%= link_to 'Editar', edit_sede_path(sede) %></td>
    <td><%= link_to 'Eliminar', sede, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
  </tr>
<% end %>
</table>

<script language="javascript" type="text/javascript"> 
  var table1_Props =  {                     
    col_3: "none", 
    col_4: "none",  
    col_5: "none", 
                };
  var tf1 = setFilterGrid("tablaSedes", table1_Props);
</script>