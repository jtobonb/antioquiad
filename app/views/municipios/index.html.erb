<div class="page-header">
        <h1>Municipios</h1>
</div>

<div class="row-fluid">
    <div class="span12">
        <%= link_to 'Ingresar un Municipio', new_municipio_path, :class => "btn btn btn-success" %>
    </div>
</div>
<br />


<table id="tablaMunicipios" class="table table-bordered table-striped">
  <tr>
    <th>nombre</th>
    <th>codigo</th>

  </tr>

<% @municipios.each do |municipio| %>
  <tr>
  <td><a href="/municipios/municipios_reporte/<%= municipio.nombre_municipio %>"><%= municipio.nombre_municipio %></a></td>
    <td><%= municipio.cod_subregion %></td>

  </tr>
<% end %>
</table>

<script language="javascript" type="text/javascript"> 
  var tf1 = setFilterGrid("tablaMunicipios");
</script>