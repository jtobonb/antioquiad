  <div class="page-header" align="center">
        <h3>Ver Programa</h3>
</div>

  <div class="row">
    <div class="span6">
      <p>
        <b>Nombre programa:</b>
        <%= @supplier.nombre_programa %>
      </p>

      <p>
        <b>Entidad ejecutora:</b>
        <%= @supplier.entidad_ejecutora %>
      </p>

      <p>
        <b>Fase:</b>
        <%= @supplier.fase %>
      </p>
    </div>

    <div class="span6">
      <p>
        <b>Fecha entrega:</b>
        <%= @supplier.fecha_entrega %>
      </p>

      <p>
        <b>Responsable:</b>
        <%= @supplier.responsable %>
      </p>

      <p>
        <b>Cantidad:</b>
        <%= @supplier.cantidad %>
      </p>
    </div>
  </div>

<br />

  <div class="row">
    <div class="span12" align="center">
      <%= link_to 'Editar', edit_supplier_path(@supplier), :class => "btn btn btn-success" %>
      <%= link_to 'Regresar', suppliers_path, :class => "btn btn btn-success" %>
    </div>
  </div>